{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"textdomain": "snow-monkey-blocks",
	"name": "snow-monkey-blocks/list",
	"title": "Icon list",
	"description": "Icons are displayed only on the actual screen.",
	"category": "smb",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "ul",
			"multiline": "li",
			"default": ""
		},
		"icon": {
			"type": "string",
			"default": "angle-right"
		},
		"iconColor": {
			"type": "string"
		}
	},
	"supports": {
		"html": false,
		"snowmonkeyeditor": {
			"blockPresets": {
				"ignore": [ "content" ]
			}
		}
	},
	"example": {
		"innerBlocks": [
			{
				"name": "snow-monkey-blocks/list-item",
				"attributes": {
					"content": "Lorem ipsum dolor sit amet"
				}
			},
			{
				"name": "snow-monkey-blocks/list-item",
				"attributes": {
					"content": "consectetur adipiscing elit"
				}
			},
			{
				"name": "snow-monkey-blocks/list-item",
				"attributes": {
					"content": "sed do eiusmod tempor incididunt ut labore et dolore magna aliqua"
				}
			}
		]
	},
	"style": "snow-monkey-blocks/list",
	"editorStyle": "snow-monkey-blocks/list/editor",
	"editorScript": "file:./index.js",
	"viewScript": "file:./script.js"
}
