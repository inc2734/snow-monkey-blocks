{
  "apiVersion": 2,
  "name": "snow-monkey-blocks/contents-outline",
	"title": "Contents outline",
	"description": "Display the table of contents above the first heading.",
  "category": "smb",
  "attributes": {
    "headings": {
      "type": "string",
      "default": "h2,h3,h4"
    },
    "moveToBefore1stHeading": {
      "type": "boolean",
      "default": true
    },
    "includesSectionTitle": {
      "type": "boolean",
      "default": false
    },
    "includesSectionHeadings": {
      "type": "boolean",
      "default": false
    },
    "anchor": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "anchor": true,
    "customClassName": false
  },
  "editorScript": "file:../../dist/block/contents-outline/editor.js"
}
