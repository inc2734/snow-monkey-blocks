    "imageHeight": {
      "type": "string",
      "source": "attribute",
      "selector": ".smb-items__item__figure > img",
      "attribute": "height",
      "default": ""
    },
    "imageSizeSlug": {
      "type": "string",
      "default": "large"
    },
    "btnLabel": {
      "type": "string",
      "source": "html",
      "selector": ".smb-items__item__btn > .smb-btn__label",
      "default": ""
    },
    "btnBackgroundColor": {
      "type": "string"
    },
    "btnTextColor": {
      "type": "string"
    },
    "btnSize": {
      "type": "string",
      "default": ""
    },
    "btnBorderRadius": {
      "type": "number"
    },
    "btnWrap": {
      "type": "boolean",
      "default": false
