{
  "name": "snow-monkey-blocks/spider-pickup-slider",
	"title": "Pickup slider",
	"description": "Display posts with pickup tags as a slider.",
  "category": "smb",
  "attributes": {
    "random": {
      "type": "boolean",
      "default": false
    },
    "postsPerPage": {
      "type": "number",
      "default": 0
    },
    "dots": {
      "type": "boolean",
      "default": false
    },
    "dotsToThumbnail": {
      "type": "boolean",
      "default": false
    },
    "arrows": {
      "type": "boolean",
      "default": true
    },
    "fade": {
      "type": "boolean",
      "default": false
    },
    "interval": {
      "type": "number",
      "default": 0
    }
  },
  "supports": {
    "align": [ "wide", "full" ]
  },
  "style": "snow-monkey-blocks/spider-pickup-slider",
  "editorScript": "file:../../dist/block/spider-pickup-slider/editor.js"
}
